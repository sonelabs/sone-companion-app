{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "credentialsSource": "local",
        "simulator": true,
        "teamId": "@TEAM_ID"
      }
    },
    "ios-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "credentialsSource": "local",
        "simulator": true,
        "teamId": "@TEAM_ID"        
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
