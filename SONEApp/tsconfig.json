{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false, 
    "jsx":"react-native", 
    "strict": true,
    "types": ["react-native"],
    "typeRoots": ["./node_modules/@types"],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "config/auth0-config.ts.bak"],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
